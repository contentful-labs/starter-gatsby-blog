{
  "extends": ["airbnb", "prettier"],
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0,
    "import/no-named-as-default-member": 0,
    "import/no-named-as-default": 0
  },
  "parserOptions": {
    "ecmaVersion": 2020
  }
}
